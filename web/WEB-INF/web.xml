<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>rpc.SearchItem</servlet-name>
        <servlet-class>rpc.SearchItem</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>rpc.SearchItem</servlet-name>
        <url-pattern>/search</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>rpc.RecommendItem</servlet-name>
        <servlet-class>rpc.RecommendItem</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>rpc.RecommendItem</servlet-name>
        <url-pattern>/recommendation</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>rpc.ItemHistory</servlet-name>
        <servlet-class>rpc.ItemHistory</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>rpc.ItemHistory</servlet-name>
        <url-pattern>/history</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>rpc.Login</servlet-name>
        <servlet-class>rpc.Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>rpc.Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>rpc.Logout</servlet-name>
        <servlet-class>rpc.Logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>rpc.Logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
</web-app>